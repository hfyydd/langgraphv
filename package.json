{
  "name": "langgraphv",
  "displayName": "LangGraph Visualizer",
  "description": "LangGraph Visualizer support for VS Code",
  "version": "0.0.7",
  "publisher": "hfloveyy",
  "engines": {
    "vscode": "^1.91.0"
  },
  "license": "MIT",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hfyydd/langgraphv.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "langgraphv.openVisualizer",
        "title": "LangGraphV: Open Langgraph Visualizer"
      },
      {
        "command": "langgraphv.newGraph",
        "title": "LangGraphV: New Graph"
      }
    ]
  },
  "scripts": {
    "compile": "concurrently \"pnpm run compile:view\" \"pnpm run compile:extension\" \"pnpm run build-css\" ",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "watch": "concurrently \"pnpm run watch:extension\" \"pnpm run watch:view\"",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build-css": "postcss src/view/tailwind.css -o out/app/styles.css"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@types/d3-selection": "^3.0.10",
    "@types/dagre": "^0.7.52",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vscode/vsce": "^3.0.0",
    "@xyflow/react": "^12.0.3",
    "dagre": "^0.8.5",
    "monaco-editor": "^0.50.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.25.1",
    "react-syntax-highlighter": "^15.5.0",
    "vsce": "^2.15.0"
  }
}